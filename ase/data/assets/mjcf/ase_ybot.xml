<mujoco model="humanoid">

  <statistic extent="2" center="0 0 1"/>

  <option timestep="0.00555"/>

  <default>
    <motor ctrlrange="-1 1" ctrllimited="true"/>
    <default class="body">
      <geom type="capsule" condim="1" friction="1.0 0.05 0.05" solimp=".9 .99 .003" solref=".015 1"/>
      <joint type="hinge" damping="0.1" stiffness="5" armature=".007" limited="true" solimplimit="0 .99 .01"/>
      <site size=".04" group="3"/>
      <default class="force-torque">
        <site type="box" size=".01 .01 .02" rgba="1 0 0 1" />
      </default>
      <default class="touch">
        <site type="capsule" rgba="0 0 1 .3"/>
      </default>
    </default>
  </default>

  <worldbody>
    <geom name="floor" type="plane" conaffinity="1" size="100 100 .2"/>
    <body name="Hips" pos="0 0 1" childclass="body">                                                                   <!-- <body name="pelvis"> -->
      <freejoint name="root"/>
      <site name="root" class="force-torque"/>
      <geom name="Hips" type="sphere" pos="0 0 0" size=".07" density="2226"/>                                          <!-- <geom name="pelvis"> -->
      <site name="Hips" class="touch" type="sphere" pos="0 0 0" size="0.091"/>                                         <!-- <site name="pelvis"> -->

      <body name="Spine" pos="-0.012273 0 0.099235">                                                                                <!-- <body name="torso"> -->
        <light name="top" pos="0 0 2" mode="trackcom"/>
        <camera name="back" pos="-3 0 1" xyaxes="0 -1 0 1 0 2" mode="trackcom"/>
        <camera name="side" pos="0 -3 1" xyaxes="1 0 0 0 1 2" mode="trackcom"/>
        <joint name="spine_x" pos="0 0 0" axis="1 0 0" range="-60 60" stiffness="1000" damping="100" armature=".02"/>  <!-- <joint name="abdomen_x"> -->
        <joint name="spine_y" pos="0 0 0" axis="0 1 0" range="-60 90" stiffness="1000" damping="100" armature=".02"/>  <!-- <joint name="abdomen_y"> -->
        <joint name="spine_z" pos="0 0 0" axis="0 0 1" range="-50 50" stiffness="1000" damping="100" armature=".02"/>  <!-- <joint name="abdomen_z"> -->
        <geom name="Spine" type="sphere" pos="0 0 0" size="0.07" density="1794"/>                                      <!-- <geom name="torso"> -->                           
        <site name="Spine" class="touch" type="sphere" pos="0 0 0" size="0.111"/>                                      <!-- <site name="torso"> -->          

        <body name="Spine1" pos="-0.014223 -0.000001 0.116454">
          <joint name="spine1_x" pos="0 0 0" axis="1 0 0" range="-60 60" stiffness="1000" damping="100" armature=".02"/>
          <joint name="spine1_y" pos="0 0 0" axis="0 1 0" range="-60 90" stiffness="1000" damping="100" armature=".02"/>
          <joint name="spine1_z" pos="0 0 0" axis="0 0 1" range="-50 50" stiffness="1000" damping="100" armature=".02"/>
          <geom name="Spine1" type="sphere" pos="0 0 0" size="0.06" density="1794"/>

          <body name="Spine2" pos="-0.016265 0.000001 0.133602">
            <joint name="spine2_x" pos="0 0 0" axis="1 0 0" range="-60 60" stiffness="1000" damping="100" armature=".02"/>
            <joint name="spine2_y" pos="0 0 0" axis="0 1 0" range="-60 90" stiffness="1000" damping="100" armature=".02"/>
            <joint name="spine2_z" pos="0 0 0" axis="0 0 1" range="-50 50" stiffness="1000" damping="100" armature=".02"/>
            <geom name="Spine2" type="sphere" pos="0 0 0" size="0.06" density="1794"/>


            <body name="Neck" pos="0.007929 0 0.150325">
              <joint name="neck_x" axis="1 0 0" range="-50 50" stiffness="100" damping="10" armature=".01"/>
              <joint name="neck_y" axis="0 1 0" range="-40 60" stiffness="100" damping="10" armature=".01"/>
              <joint name="neck_z" axis="0 0 1" range="-45 45" stiffness="100" damping="10" armature=".01"/>
              <geom name="Neck" type="sphere" pos="0 0 0" size="0.095" density="1081"/>
              <site name="Neck" class="touch" pos="0 0 0" type="sphere" size="0.103"/>
              <camera name="egocentric" pos=".103 0 0.175" xyaxes="0 -1 0 .1 0 1" fovy="80"/>
            </body>

            <body name="RightShoulder" pos="0.007056 -0.061057 0.091105">
              <joint name="right_shoulder_x" axis="1 0 0" range="-180 45" stiffness="400" damping="40" armature=".02"/>
              <joint name="right_shoulder_y" axis="0 1 0" range="-180 60" stiffness="400" damping="40" armature=".02"/>
              <joint name="right_shoulder_z" axis="0 0 1"  range="-90 90" stiffness="400" damping="40" armature=".02"/>
              <geom name="RightShoulder" fromto="0 -0.05 0 0 -0.12 0" size=".03" density="982"/>
              
              <body name="RightArm" pos="-0.026009 -0.126550 -0.002660">
                <joint name="right_arm_x" axis="1 0 0" range="-180 45" stiffness="400" damping="40" armature=".02"/>
                <joint name="right_arm_y" axis="0 1 0" range="-180 60" stiffness="400" damping="40" armature=".02"/>
                <joint name="right_arm_z" axis="0 0 1"  range="-90 90" stiffness="400" damping="40" armature=".02"/>
                <geom name="RightArm" fromto="0 -0.05 0  0 -0.27 0" size=".03" density="982"/>

                <body name="RightForeArm" pos="0.0 -0.274047 0.0">
                  <joint name="right_elbow" axis="0 1 0" range="-160 0" stiffness="300" damping="30" armature=".01"/>
                  <geom name="RightForeArm" fromto="0 -0.0525 0 0 -0.20 0" size="0.03" density="1056"/>
                  <site name="RightForeArm" class="touch" pos="0 0 -0.12" size="0.041 0.0685" zaxis="0 1 0"/>

                  <body name="RightHand" pos="0.0 -0.276145 0.0">
                    <joint name="right_hand_x" axis="1 0 0" range="-180 45" stiffness="400" damping="40" armature=".02"/>
                    <joint name="right_hand_y" axis="0 1 0" range="-180 60" stiffness="400" damping="40" armature=".02"/>
                    <joint name="right_hand_z" axis="0 0 1"  range="-90 90" stiffness="400" damping="40" armature=".02"/>
                    <geom name="RightHand" type="sphere" size=".04" density="1865"/>
                    <site name="RightHand" class="touch" type="sphere" size=".041"/>
                  </body>   <!-- RightHand -->
                </body>     <!-- RightForeArm -->
              </body>       <!-- RightArm -->
            </body>         <!-- RightShoulder -->

            <body name="LeftShoulder" pos="0.007056 0.061058 0.091104">
              <joint name="left_shoulder_x" axis="1 0 0" range="-180 45" stiffness="400" damping="40" armature=".02"/>
              <joint name="left_shoulder_y" axis="0 1 0" range="-180 60" stiffness="400" damping="40" armature=".02"/>
              <joint name="left_shoulder_z" axis="0 0 1"  range="-90 90" stiffness="400" damping="40" armature=".02"/>
              <geom name="LeftShoulder" fromto="0 0.05 0 0 0.12 0" size=".03" density="982"/>
              
              <body name="LeftArm" pos="-0.026009 0.126550 -0.002659">
                <joint name="left_arm_x" axis="1 0 0" range="-180 45" stiffness="400" damping="40" armature=".02"/>
                <joint name="left_arm_y" axis="0 1 0" range="-180 60" stiffness="400" damping="40" armature=".02"/>
                <joint name="left_arm_z" axis="0 0 1"  range="-90 90" stiffness="400" damping="40" armature=".02"/>
                <geom name="LeftArm" fromto="0 0.05 0  0 0.27 0" size=".03" density="982"/>

                <body name="LeftForeArm" pos="0.0 0.274047 0.0">
                  <joint name="left_elbow" axis="0 1 0" range="-160 0" stiffness="300" damping="30" armature=".01"/>
                  <geom name="LeftForeArm" fromto="0 0.0525 0 0 0.20 0" size="0.03" density="1056"/>
                  <site name="LeftForeArm" class="touch" pos="0 0 0.12" size="0.041 0.0685" zaxis="0 1 0"/>

                  <body name="LeftHand" pos="0.0 0.276145 0.0">
                    <joint name="left_hand_x" axis="1 0 0" range="-180 45" stiffness="400" damping="40" armature=".02"/>
                    <joint name="left_hand_y" axis="0 1 0" range="-180 60" stiffness="400" damping="40" armature=".02"/>
                    <joint name="left_hand_z" axis="0 0 1"  range="-90 90" stiffness="400" damping="40" armature=".02"/>
                    <geom name="LeftHand" type="sphere" size=".04" density="1865"/>
                    <site name="LeftHand" class="touch" type="sphere" size=".041"/>
                  </body>   <!-- LeftHand -->
                </body>     <!-- LeftForeArm -->
              </body>       <!-- LeftArm -->
            </body>         <!-- LeftShoulder -->

          </body>           <!-- Spine2 -->
        </body>             <!-- Spine1 -->
      </body>               <!-- Spine -->

      <body name="LeftUpLeg" pos="-0.000554 0.091245 -0.066564">
        <site name="left_hip" class="force-torque"/>
        <joint name="left_hip_x" axis="1 0 0" range="-60 15" stiffness="500" damping="50" armature=".02"/>
        <joint name="left_hip_y" axis="0 1 0" range="-140 60" stiffness="500" damping="50" armature=".02"/>
        <joint name="left_hip_z" axis="0 0 1" range="-60 35" stiffness="500" damping="50" armature=".02"/>
        <geom name="LeftUpLeg" fromto="0 0 -0.06 0 0 -0.36" size="0.055" density="1269"/>
        <site name="LeftUpLeg" class="touch" pos="0 0 -0.21" size="0.056 0.301" zaxis="0 0 -1"/>

        <body name="LeftLeg" pos="-0.005170 0.002447 -0.405954">
          <site name="left_knee" class="force-torque" pos="0 0 0"/>
          <joint name="left_knee" pos="0 0 0" axis="0 1 0" range="0 160" stiffness="500" damping="50" armature=".02"/>
          <geom name="LeftLeg" fromto="0 0 -0.045 0 0 -0.355"  size=".05" density="1014"/>
          <site name="LeftLeg" class="touch" pos="0 0 -0.2" size="0.051 0.156" zaxis="0 0 -1"/>

          <body name="LeftFoot" pos="-0.020576 -0.002447 -0.420480">
            <site name="left_ankle" class="force-torque"/>
            <joint name="left_ankle_x" pos="0 0 0" axis="1 0 0" range="-30 30" stiffness="400" damping="40" armature=".01"/>
            <joint name="left_ankle_y" pos="0 0 0" axis="0 1 0" range="-55 55" stiffness="400" damping="40" armature=".01"/>
            <joint name="left_ankle_z" pos="0 0 0" axis="0 0 1" range="-40 40" stiffness="400" damping="40" armature=".01"/>
            <geom name="LeftFoot" type="box" pos="0.045 0 -0.0225" size="0.0885 0.045 0.0275" density="1141"/>
            <site name="LeftFoot" class="touch" type="box" pos="0.045 0 -0.0225" size="0.0895 0.055 0.0285"/>
          </body>           <!-- LeftFoot -->
        </body>             <!-- LeftLeg -->
      </body>               <!-- LeftUpLeg -->

      <body name="RightUpLeg" pos="-0.000554 -0.091245 -0.066564">
        <site name="right_hip" class="force-torque"/>
        <joint name="right_hip_x" axis="1 0 0" range="-60 15" stiffness="500" damping="50" armature=".02"/>
        <joint name="right_hip_y" axis="0 1 0" range="-140 60" stiffness="500" damping="50" armature=".02"/>
        <joint name="right_hip_z" axis="0 0 1" range="-60 35" stiffness="500" damping="50" armature=".02"/>
        <geom name="RightUpLeg" fromto="0 0 -0.06 0 0 -0.36" size="0.055" density="1269"/>
        <site name="RightUpLeg" class="touch" pos="0 0 -0.21" size="0.056 0.301" zaxis="0 0 -1"/>

        <body name="RightLeg" pos="-0.005145 -0.002447 -0.405954">
          <site name="right_knee" class="force-torque" pos="0 0 0"/>
          <joint name="right_knee" pos="0 0 0" axis="0 1 0" range="0 160" stiffness="500" damping="50" armature=".02"/>
          <geom name="RightLeg" fromto="0 0 -0.045 0 0 -0.355"  size=".05" density="1014"/>
          <site name="RightLeg" class="touch" pos="0 0 -0.2" size="0.051 0.156" zaxis="0 0 -1"/>

          <body name="RightFoot" pos="-0.020602 0.002447 -0.420479">
            <site name="right_ankle" class="force-torque"/>
            <joint name="right_ankle_x" pos="0 0 0" axis="1 0 0" range="-30 30" stiffness="400" damping="40" armature=".01"/>
            <joint name="right_ankle_y" pos="0 0 0" axis="0 1 0" range="-55 55" stiffness="400" damping="40" armature=".01"/>
            <joint name="right_ankle_z" pos="0 0 0" axis="0 0 1" range="-40 40" stiffness="400" damping="40" armature=".01"/>
            <geom name="RightFoot" type="box" pos="0.045 0 -0.0225" size="0.0885 0.045 0.0275" density="1141"/>
            <site name="RightFoot" class="touch" type="box" pos="0.045 0 -0.0225" size="0.0895 0.055 0.0285"/>
          </body>           <!-- RightFoot -->
        </body>             <!-- RightLeg -->
      </body>               <!-- RightUpLeg -->

    </body>                 <!-- Hips -->
  </worldbody>

